public function GetShipAddr(shipAddr1 as String, ShipAddr2 as String, ShipAddr3 as String, ShipPostcode as string, ShipCity as string, ShipState as string, ShipCountry as string ) As string
     shipAddrVar = shipAddr1 

     If Not IsDBNull( ShipAddr2 ) And ShipAddr2  &lt;&gt; "" Then
            shipAddrVar = shipAddrVar &amp;  VbCrLf  &amp;   ShipAddr2 
     End If

     If Not IsDBNull( ShipAddr3  ) And ShipAddr3 &lt;&gt; "" Then
           shipAddrVar = shipAddrVar &amp; VbCrLf  &amp;   ShipAddr3 
     End If

    If Not IsDBNull( ShipPostcode ) And ShipPostcode &lt;&gt; "" Then
           shipAddrVar = shipAddrVar &amp; VbCrLf &amp;  ShipPostcode 
     End If

    If Not IsDBNull( ShipCity ) And ShipCity &lt;&gt; "" Then
           shipAddrVar = shipAddrVar &amp; " " &amp; ShipCity 
     End If

    If Not IsDBNull( ShipState ) And ShipState &lt;&gt; "" Then
           shipAddrVar = shipAddrVar &amp; VbCrLf &amp;  ShipState 
     End If

    If Not IsDBNull( ShipCity ) And ShipCity &lt;&gt; "" Then
           shipAddrVar = shipAddrVar &amp; " " &amp; ShipCountry 
     End If

return shipAddrVar 

End function


