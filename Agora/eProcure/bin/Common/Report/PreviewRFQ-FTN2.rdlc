     End If

    If Not IsDBNull( suppState ) And suppState &lt;&gt; "" Then
           suppAddrVar  = suppAddrVar  &amp; VbCrLf &amp;  suppState 
     End If

    If Not IsDBNull( suppCountry ) And suppCountry &lt;&gt; "" Then
