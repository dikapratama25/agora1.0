public function GetBuyerAddr(buyerAddr1 as String, buyerAddr2 as String, buyerAddr3 as String, buyerPostcode as string, buyerCity as string, buyerState as string, buyerCountry as string ) As string
     buyerAddrVar = buyerAddr1 

     If Not IsDBNull( buyerAddr2 ) And buyerAddr2 &lt;&gt; "" Then
            buyerAddrVar = buyerAddrVar &amp;  VbCrLf  &amp;   buyerAddr2 
     End If

     If Not IsDBNull( buyerAddr3 ) And buyerAddr3 &lt;&gt; "" Then
           buyerAddrVar = buyerAddrVar &amp; VbCrLf  &amp;   buyerAddr3 
     End If

    If Not IsDBNull( buyerPostcode ) And buyerPostcode &lt;&gt; "" Then
           buyerAddrVar = buyerAddrVar &amp; VbCrLf &amp;  buyerPostcode 
     End If

    If Not IsDBNull( buyerCity ) And buyerCity &lt;&gt; "" Then
           buyerAddrVar = buyerAddrVar &amp; " " &amp; buyerCity 
     End If

    If Not IsDBNull( buyerState ) And buyerState &lt;&gt; "" Then
           buyerAddrVar = buyerAddrVar &amp; VbCrLf &amp;  buyerState 
     End If

    If Not IsDBNull( buyerCountry ) And buyerCountry &lt;&gt; "" Then
           buyerAddrVar = buyerAddrVar &amp; " " &amp; buyerCountry 
     End If

return buyerAddrVar 

End function

public function GetBuyerComInfor(buyerBussRegNo as String, buyerCRName as String, buyerTel as String, buyerEmail as string) As string
     buyerComInforVar = VbCrLf  &amp; "Business Reg. No."  &amp; space(6) &amp; ":  "  &amp; buyerBussRegNo 
     buyerComInforVar = buyerComInforVar  &amp; VbCrLf  &amp; "Contact Person"  &amp; space(11) &amp; ":  "  &amp; buyerCRName 
     buyerComInforVar = buyerComInforVar  &amp; VbCrLf  &amp; "Tel"  &amp; space(31) &amp; ":  "  &amp; buyerTel 
     buyerComInforVar = buyerComInforVar  &amp; VbCrLf  &amp; "Email"  &amp; space(27) &amp; ":  "  &amp; buyerEmail 
    

    return buyerComInforVar 

End function

