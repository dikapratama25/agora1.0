     End If

return suppAddrVar  

End function

public function GetSuppComInfor(suppBussRegNo as String, suppTel as String, suppEmail as string) As string
     suppComInforVar  = VbCrLf  &amp; "Business Reg. No."  &amp; space(6) &amp; ":  "  &amp; suppBussRegNo 

      
